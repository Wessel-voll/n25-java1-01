package br.com.senaisp.bauru.classes;

public class Estudante {
	//Fields ou campos ou variáveis de instância (Mais comum Fields e campos)
	private String nome;
	private int ra;
	private double nota;
	private boolean ehFumante;
	//Constructor
	public Estudante() {
		nome = "Sem nome";
		ra = 0;
		nota = 0.0;
		ehFumante = false;
	}
}
